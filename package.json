    "tslib": "^2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-builders/jest": "13.0.4",
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular-eslint/builder": "13.2.1",
